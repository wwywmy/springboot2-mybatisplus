<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.abc.springboot2.mybatisplus.mapper.TeacherMapper">
	<select id="selectByNo" parameterType="java.lang.String"
		resultType="com.abc.springboot2.mybatisplus.entity.Teacher">
		select * from teacher where no = #{no}
	</select>
</mapper>
